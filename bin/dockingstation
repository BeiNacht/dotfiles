#!/bin/sh
xrandr --output VIRTUAL1 --off --output eDP1 --off --output DP1 --mode 3840x2160 --pos 0x0 --rotate normal --output DP2-1 --mode 3840x2160 --pos 3840x0 --rotate normal --output DP2-2 --off --output DP2-3 --off --output HDMI2 --off --output HDMI1 --off --output VGA1 --off --output DP2 --off
