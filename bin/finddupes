    -path "*/node_modules" \
    -o -path "*/.cache" \
    -o -path "*/.kodi" \
    -o -path "*/.config" \
    -o -path "*/.node-gyp" \
    -o -path "*/.jd" \
    -o -path "*/.themes" \
    -o -path "*/.gem" \
    -o -path "*/.git" \
    -o -path "*/.tldr" \
    -o -path "*/.viking-maps" \
    -o -path "*/Workspace" \
    -o -path "*/.vscode" \
    -o -path "*/.local/share" \
    -o -path "*/.dotfiles" \
    -o -path "*/.ssh" \
    \) -prune -o


